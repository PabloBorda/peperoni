<table class="autocomplete-element" width="100%" height>
  <tr>
    <td width="100%">
      <table>
        <tr>
          <td>
            <input type="image" alt="FORK!" src="/images/fork.jpg" name="fork" onclick="fork_note(<%=data[1]%>,<%=data[0]%>)"/>
          </td>
        </tr>
        <tr>
          <td class="item_content">
            <input type="image" src="/images/fork.jpg" name="fork" onclick="view_ticket(<%=data[1]%>,<%=data[0]%>)"/>
          </td>
        </tr>
      </table>
    </td> 
    <td width="100%">
      <span class="knows">
        <%= data[2] %>
      </span>
    </td>
    <td width="100%" class="pics" align="right">
    <% if (data[3].eql? "JIRA")                                      %>
    <%=  "<img onerror=\"imgError(this)\" src=\"/images/jira-icon.jpg\">"  %>
    <% else                                                          %>
    <%=  "<img onerror=\"imgError(this)\" src=\"http://myhub.corp.ebay.com/User%20Photos/Profile%20Pictures/_w/corp_#{data[3]}_MThumb_jpg.jpg\"/>" %>
    <% end                                                           %>
    </td>
  </tr>
</table>