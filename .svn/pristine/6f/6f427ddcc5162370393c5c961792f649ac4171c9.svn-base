


<style type="text/css">

.knows {
  font-size: 10px !important;
}



.autocomplete-element{
    height: 100px;
    overflow-y: auto;
}



</style>

<script type="text/javascript">

function imgError(image) {
    image.onerror = "";
    image.src = "/images/nopic.png";
    return true;
}


</script>



<table class="autocomplete-element" width="100%">
  <tr>
    <td width="60px">
      <table>
        <tr>
          <td>
            <input type="image" alt="FORK!" src="/images/fork.jpg" name="fork" onclick="fork_note(<%=data[1]%>,<%=data[0]%>)"/>
          </td>
        </tr>
        <tr>
          <td class="item_content">
            <input type="image" src="/images/fork.jpg" name="fork" onclick="view_ticket(<%=data[0]%>,<%=data[1]%>)"/>
          </td>
        </tr>
      </table>
    </td> 
    <td width="200px">
      <span class="knows">
        <%= data[2] %>
      </span>
    </td>
    <td width="100%" class="pics" right="0%">

      <table>


        <tr align="center">
          <td>
            <span class="starRating">
              <input id="rating5" type="radio" name="rating" value="5">
              <label for="rating5">5</label>
              <input id="rating4" type="radio" name="rating" value="4">
              <label for="rating4">4</label>
              <input id="rating3" type="radio" name="rating" value="3">
              <label for="rating3">3</label>
              <input id="rating2" type="radio" name="rating" value="2">
              <label for="rating2">2</label>
              <input id="rating1" type="radio" name="rating" value="1">
              <label for="rating1">1</label>
            </span>
            
          </td>

        </tr>



        <tr align="center">
          <td>
     <!--       <% if (data[3] == "JIRA") { %>
              <img onerror="imgError(this)" src="/images/jira-icon.jpg"/>
            <% } else { %>
            <img onerror="imgError(this)" src="http://myhub.corp.ebay.com/User%20Photos/Profile%20Pictures/_w/corp_<%=data[3]%>_MThumb_jpg.jpg"/>
<% }  %>
-->
          </td>

        </tr>

      </table>
      
    </td>
  </tr>
</table>